{
  "name": "create-fict",
  "version": "0.1.0",
  "description": "Scaffolder for Fict Kit projects",
  "type": "module",
  "bin": {
    "create-fict": "bin/create-fict.js"
  },
  "files": [
    "bin",
    "dist",
    "templates"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fictjs/kit.git",
    "directory": "packages/create-fict"
  },
  "scripts": {
    "build": "tsup",
    "prepack": "pnpm run build",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "tsup": "^8.5.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "keywords": ["fict", "scaffold", "create"],
  "license": "MIT",
  "author": "unadlib"
}
